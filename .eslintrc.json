{
  "parser": "babel-eslint",
  "extends": "google",
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "node": true
  },
  "rules": {
    "eol-last": 0,
    "no-undef": 0,
    "object-curly-spacing": [ 2, "always" ],
    "array-bracket-spacing": [ 2, "always" ],
    "computed-property-spacing": [ 2, "always" ],
    "indent": [ 2, 4 ],
    "no-multi-spaces": 0,
    "no-mixed-requires": 0,
    "one-var": [
      2,
      {
        "var": "always",
        "let": "always",
        "const": "never"
      }
    ],
    "new-cap": 0,
    "padded-blocks": 0,
    "valid-jsdoc": 0
  }
}